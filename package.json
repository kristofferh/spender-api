    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "ndb": "^1.0.42",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "sequelize-cli": "^3.2.0"
