    "graphql-sequelize": "^9.0.1",
    "jsonwebtoken": "^8.1.0",
    "morgan": "^1.9.1",
    "multer": "^1.3.0",
    "mysql2": "^1.4.2",
    "passwordless": "^1.1.2",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.35.1"
